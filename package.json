{
	"name": "dracula-highlighting",
	"displayName": "Dracula Highlighting",
	"version": "0.0.3",
	"description": "Modern dracula-highlighting themes for VSCode.",
	"categories": [
		"Themes"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/Mopsgamer/dracula-highlighting.git"
	},
	"publisher": "Mopsgamer",
	"scripts": {
		"fmt": "oxfmt",
		"prepublish": "bun run scripts/assets.js",
		"package": "vsce package"
	},
	"devDependencies": {
		"oxfmt": "^0.24.0",
		"sharp": "^0.34.5",
		"vsce": "^2.15.0"
	},
	"contributes": {
		"themes": [
			{
				"label": "Dark Modern (Dracula)",
				"uiTheme": "vs-dark",
				"path": "./themes/dark-modern-dracula.json"
			},
			{
				"label": "Dark (Dracula)",
				"uiTheme": "vs-dark",
				"path": "./themes/dark-dracula.json"
			},
			{
				"label": "Monokai (Dracula)",
				"uiTheme": "vs-dark",
				"path": "./themes/monokai-dracula.json"
			},
			{
				"label": "Monokai Dimmed (Dracula)",
				"uiTheme": "vs-dark",
				"path": "./themes/monokai-dimmed-dracula.json"
			},
			{
				"label": "Solarized Dark (Dracula)",
				"uiTheme": "vs-dark",
				"path": "./themes/solarized-dark-dracula.json"
			}
		]
	},
	"icon": "assets/icon.png",
	"engines": {
		"vscode": "^1.70.0"
	}
}
